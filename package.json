    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "concurrently": "^4.0.1",
    "express": "^4.16.3",
    "if-env": "^1.0.4",
    "material-icons-react": "^1.0.2",
    "mongoose": "^5.2.13",
    "nodemon": "^1.18.4",
    "react-materialize": "^2.4.5",
